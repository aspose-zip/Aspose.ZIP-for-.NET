
@model  LandingPageModel

@{
	Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="container-fluid header1 header2 minify-header appfamily-header">
	<div class="container">
		<div class="row">
			<div class="col-md-8">
				<h1>Archive File Format Solution</h1>
				<h4>On Premise APIs and free apps offering features to compress & decompress ZIP, TAR, GZIP, BZ2 file formats.</h4>
			</div>
			<div class="col-md-4 tc">
				<img class=" col-xs-12" src="../img/aspose_zip-brand.png" alt="Aspose.ZIP" width="320" />
			</div>
		</div>
	</div>
</div>
<div class="container-fluid productfamilypage bg-white pdb-5">
	<div class="row">
		<div class="container tc">
			<div class="clearfix">&nbsp;</div>
			<div class="clearfix">&nbsp;</div>
			<h3>@string.Format(@Model.Resources["AsposeProductFamilyInclude"], @Model.Resources["AsposeZIP"])  </h3>
			<div class="clearfix">&nbsp;</div>
			<div class="clearfix">&nbsp;</div>

			<div class="col-lg-4 productfamily_box">
				<div class="productfamilytitle">
					<div class="imgblock">
						<img src="../img/aspose_zip-app-.png" alt="Aspose.ZIP for .NET" />
					</div>
					<a href="/zip/zip-file"><span class="spanclass">@Model.Resources["AsposeZIP"] @Model.Resources["AsposeProductFor"] <em>Zip Files</em></span><em>Free App offering features to Compress files to create simple or protected zip archives from any OS, on any browser.</em></a>
				</div>
			</div>
			<div class="col-lg-4 productfamily_box">
				<div class="productfamilytitle">
					<div class="imgblock">
						<img src="../img/aspose_zip-app-.png" alt="Aspose.ZIP for .NET" />
					</div>
					<a href="/zip/unzip-file"><span class="spanclass">@Model.Resources["AsposeZIP"] @Model.Resources["AsposeProductFor"] <em>UnZip Files</em></span><em>Uncompress zip archives to get individual files without leaving your favorite browser.</em></a>
				</div>
			</div>
		</div>
	</div>
</div>
<div class="spacer">&nbsp;</div>
